{
  "name": "@lealt/kaori",
  "version": "1.4.0",
  "description": "A feature-rich library for Discord.js.",
  "author": "Lealt",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier . --write",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "discord.js": "^14.24.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "keywords": [
    "discord",
    "discord.js",
    "ui",
    "utils",
    "components",
    "builder",
    "interaction",
    "discord-bot",
    "componentsV2",
    "kaori"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lealtt/kaori.git"
  },
  "bugs": {
    "url": "https://github.com/lealtt/kaori/issues"
  },
  "homepage": "https://github.com/lealtt/kaori#readme"
}
